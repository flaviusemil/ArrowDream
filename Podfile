# Uncomment the next line to define a global platform for your project
# platform :ios, '10.0'

target 'ArrowDream' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for ArrowDream
 
  pod 'Firebase/Auth'
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'
  pod 'FBSDKShareKit'
  pod 'GoogleSignIn'

  target 'ArrowDreamTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'ArrowDreamUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
